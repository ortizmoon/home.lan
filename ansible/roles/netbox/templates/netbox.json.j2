{
  "ID": "netbox",
  "Name": "netbox",
  "Tags": [
    "traefik.enable=true",
    "traefik.http.routers.netbox.rule=Host(`{{ netbox_domain }}`)",
    "traefik.http.routers.netbox.entrypoints=web",
    "traefik.http.services.netbox.loadbalancer.server.port=8000"
  ],
  "Address": "{{ hostvars[inventory_hostname]['ansible_host'] }}",
  "Port": 8000,
  "Check": {
    "HTTP": "http://{{ hostvars[inventory_hostname]['ansible_host'] }}:8000/login/",
    "Interval": "10s"
  }
}