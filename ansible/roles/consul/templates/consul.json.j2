{
  "ID": "consul",
  "Name": "consul",
  "Tags": [
    "traefik.enable=true",
    "traefik.http.routers.consul.rule=Host(`{{ consul_domain }}`)",
    "traefik.http.routers.consul.entrypoints=web",
    "traefik.http.services.consul.loadbalancer.server.port=8500"
  ],
  "Address": "{{ hostvars[inventory_hostname]['ansible_host'] }}",
  "Port": 8500,
  "Check": {
    "HTTP": "http://{{ hostvars[inventory_hostname]['ansible_host'] }}:8500/ui/",
    "Interval": "10s"
  }
}